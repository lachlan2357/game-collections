{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 4,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "useTabs": true,
  "vueIndentScriptAndStyle": true,
  "order": "alphabetical"
}